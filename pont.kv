MDScreen:
    id: screen
    theme_bg_color: "Custom"
    md_bg_color: 0.08627450980392157, 0.08627450980392157, 0.08627450980392157, 1

    MDFloatLayout:
        pos_hint:{"center_x": .5, "center_y": .45}
        adaptive_size: True
        size:"350dp","450dp"
        MDScrollView:
            do_scroll_x: False
            pos_hint: {"center_x": .5, "center_y": .5}        
            MDBoxLayout:
                id: main_scroll
                orientation: "vertical"
                adaptive_height: True

                MDBoxLayout:
                    adaptive_height: True
                    MDBoxLayout:
                        orientation: "vertical"
                        adaptive_height: True
                        MDList:
                            id: box
                            adaptive_size: True
                            theme_bg_color: "Custom"
                            spacing: "4dp"
                            pos_hint: {"center_x": .5, "center_y": .5}        


    MDFloatLayout:
        radius: [12]
        pos_hint:{"center_x": .5, "center_y": 0.06}
        adaptive_size: True
        size:"350dp","70dp"
        theme_bg_color:'Custom'
        md_bg_color:0.1068627450980392, 0.1068627450980392, 0.1068627450980392, 1
        
        MDFabButton:
            icon: "plus"
            style: "standard"
            theme_bg_color: "Custom"
            md_bg_color: 0.07450980392156863, 0.7803921568627451, 0.5098039215686274, 1
            pos_hint: {"center_x": .8, "center_y": .5}
            on_press: app.num()


        MDFabButton:
            icon: "pencil"
            style: "standard"
            theme_bg_color: "Custom"
            md_bg_color: 0.07450980392156863, 0.7803921568627451, 0.5098039215686274, 1
            pos_hint: {"center_x": .2, "center_y": .5}
            on_press: app.sp()